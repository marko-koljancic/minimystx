{
  "name": "minimystx",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "description": "A web-based 3D parametric modeling tool.",
  "keywords": [
    "parametric-design",
    "3d-modeling",
    "node-editor",
    "webgl",
    "three.js",
    "react",
    "typescript",
    "webassembly",
    "rust",
    "cad",
    "procedural-modeling",
    "computational-design",
    "grasshopper-like",
    "browser-based",
    "client-side",
    "real-time-3d",
    "lighting-design",
    "geometry-nodes",
    "visual-programming",
    "architectural-design",
    "creative-coding"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "build-all": "npm run build-core && npm run build",
    "build-core": "wasm-pack build src/wasm --target web --out-dir ./pkg/minimystx-core-wasm --release",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "dependencies": {
    "@dagrejs/dagre": "^1.1.4",
    "@react-three/drei": "^10.5.0",
    "@react-three/fiber": "^9.1.4",
    "@xyflow/react": "^12.7.0",
    "elkjs": "^0.9.3",
    "fflate": "^0.8.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-icons": "^5.5.0",
    "react-router": "^7.6.3",
    "three": "^0.178.0",
    "uuid": "^11.1.0",
    "web-ifc": "^0.0.69",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/three": "^0.178.0",
    "@typescript-eslint/eslint-plugin": "^7.13.1",
    "@typescript-eslint/parser": "^7.13.1",
    "@vitejs/plugin-react": "^4.3.1",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "typescript": "^5.2.2",
    "vite": "^5.3.1"
  }
}
